<UserControl x:Class="WpfInstanceInspector.InstanceInspectorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfInstanceInspector"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <Style TargetType="StackPanel">
            <Setter Property="Background" Value="Gray"></Setter>
        </Style>
    </UserControl.Resources>

    <StackPanel Orientation="Vertical" x:Name="sp_PropsPanel" HorizontalAlignment="Left" VerticalAlignment="Top" />


</UserControl>
